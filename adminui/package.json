{
    "name": "react-dashboard",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "apexcharts": "^3.30.0",
        "boxicons": "^2.0.9",
        "npm-watch": "^0.11.0",
        "react": "^17.0.2",
        "react-apexcharts": "^1.3.9",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux": "^4.1.2",
        "serve": "^13.0.2",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-scripts --openssl-legacy-provider start",
        "dev": "PORT=5002 react-scripts start",
        "build": "react-scripts build",
        "watch": "npm-watch",
        "test": "react-scripts test",
        "serve": "PORT=5002 serve -s build",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx"
        }
    }
}
{
    "name": "react-dashboard",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@material-ui/icons": "^4.11.2",
        "express": "^4.17.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.0",
        "react-toastify": "^8.1.1",
        "sass": "^1.49.7"
    },
    "scripts": {
        "start": "react-scripts --openssl-legacy-provider start",
        "dev": "PORT=5001 react-scripts start",
        "build": "react-scripts build",
        "watch": "npm-watch",
        "test": "react-scripts test",
        "serve": "PORT=5001 serve -s build",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not ie <= 11",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx"
        }
    }
}